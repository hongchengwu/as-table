    "lint-staged": "^7.2.2",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "url-loader": "^0.5.8",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
